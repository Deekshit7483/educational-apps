			android:weightSum="1"
			android:layout_weight="1">
			<LinearLayout
				android:id="@+id/b1"
				android:layout_width="match_parent"
				android:layout_height="45dp"
				android:gravity="center_horizontal|center_vertical"
				android:orientation="horizontal">
